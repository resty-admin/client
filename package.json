{
	"name": "client",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"dev:ssr": "ng run client:serve-ssr",
		"serve:ssr": "node dist/client/server/main.js",
		"build:ssr": "ng build && ng run client:server",
		"build:dev": "ng build --configuration=dev",
		"build:prod": "ng build --configuration=production",
		"prerender": "ng run client:prerender",
		"update-packages": "yarn-check -u",
		"husky:prepare": "husky install && chmod ug+x .husky/*",
		"lint:check": "ng lint",
		"lint:fix": "yarn lint:check --fix",
		"lint:staged": "eslint --fix",
		"slint:check": "stylelint **/*.scss",
		"slint:fix": "yarn slint:check --fix",
		"slint:staged": "stylelint --fix",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"compodoc:build": "compodoc -p tsconfig.doc.json",
		"compodoc:serve": "compodoc -s",
		"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
		"storybook": "yarn docs:json && start-storybook -p 6006",
		"build-storybook": "yarn docs:json && build-storybook",
		"codegen": "graphql-codegen --config codegen.ts",
		"i18n:extract": "transloco-keys-manager extract --unflat",
		"i18n:find": "transloco-keys-manager find"
	},
	"private": true,
	"dependencies": {
